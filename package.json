{
  "name": "austin-traffic-incidents",
  "version": "1.0.0",
  "description": "Real time Austin, Texas traffic incidents",
  "main": "index.js",
  "scripts": {
    "aws:creds": "yarn tsc scripts/awsCreds.ts && node scripts/awsCreds.js && yarn fresh:scripts",
    "aws:init": "yarn tsc scripts/awsInit.ts && node scripts/awsInit.js && yarn fresh:scripts",
    "aws:deploy:ui": "yarn tsc scripts/deployUi.ts && node scripts/deployUi.js && yarn fresh:scripts",
    "build": "./node_modules/.bin/webpack --env.prod",
    "danger:ci": "./node_modules/.bin/danger ci --dangerfile=scripts/dangerfile.ts",
    "danger": "./node_modules/.bin/danger local --dangerfile=scripts/dangerfile.ts",
    "fresh": "rm -rf node_modules build reports coverage",
    "fresh:scripts": "rm -rf scripts/*.js",
    "lint:ci": "yarn lint --format junit --out reports/tslint.xml",
    "lint:fix": "yarn lint --fix",
    "lint": "./node_modules/.bin/tslint --project '.'",
    "test:coverage:ci": "yarn test --coverage --no-cache --maxWorkers=6 --ci",
    "test:coverage": "yarn test --coverage --no-cache --maxWorkers=6",
    "test:ci": "yarn test --ci --reporters=default --reporters=jest-junit",
    "test:watch": "yarn test --watch",
    "run:lambda": "amplify function invoke crashFunction",
    "test": "./node_modules/.bin/jest",
    "check": "yarn lint && yarn test:coverage",
    "start": "./node_modules/.bin/webpack-dev-server --env.dev --hot",
    "tsc": "./node_modules/.bin/tsc --lib esnext,dom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amaingot/austin-traffic-incidents.git"
  },
  "keywords": [
    "react",
    "graphql"
  ],
  "author": "Alex Maingot (alex@maingot.us)",
  "license": "GPL-3.0",
  "targets": {
    "chrome": "58",
    "ie": "11"
  },
  "bugs": {
    "url": "https://github.com/amaingot/austin-traffic-incidents/issues"
  },
  "homepage": "https://github.com/amaingot/austin-traffic-incidents#readme",
  "dependencies": {
    "apollo-client": "^2.5.1",
    "aws-amplify": "^1.1.26",
    "aws-appsync": "^1.7.2",
    "aws-appsync-react": "^1.2.7",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@aws-amplify/cli": "^1.6.5",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/types": "^7.4.4",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/webpack": "^4.4.27",
    "aws-sdk": "^2.444.0",
    "babel-loader": "^8.0.5",
    "danger": "^7.1.3",
    "immutable": "^4.0.0-rc.12",
    "jest": "^24.7.1",
    "jest-environment-jsdom": "^24.7.1",
    "jest-junit": "^6.3.0",
    "prettier": "^1.17.0",
    "react-hot-loader": "^4.8.4",
    "react-testing-library": "^7.0.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "typescript-tslint-plugin": "^0.3.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "testEnvironment": "jsdom"
  }
}
